{
  "name": "bueadelights",
  "version": "1.0.0",
  "description": "Local Flavors at Your Fingertips - Cameroonian Cuisine Delivery",
  "main": "index.js",
  "scripts": {
    "build-css": "tailwindcss -i ./static/css/input.css -o ./static/css/output.css --watch",
    "build-css-prod": "tailwindcss -i ./static/css/input.css -o ./static/css/output.css --minify",
    "dev": "concurrently \"npm run build-css\" \"python manage.py runserver\"",
    "collect-static": "python manage.py collectstatic --noinput"
  },
  "keywords": [
    "django",
    "tailwind",
    "e-commerce",
    "food-delivery",
    "cameroon",
    "buea"
  ],
  "author": "BueaDelights Team",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.3.6"
  },
  "dependencies": {
    "alpinejs": "^3.13.3"
  }
}